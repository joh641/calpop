{
  "apiVersion": "1.0",
  "swaggerVersion": "1.2",
  "basePath": "http://calpop.herokuapp.com",
  "resourcePath": "/populate",
  "apis": [
    {
      "path": "/populate.json",
      "operations": [
        {
          "summary": "Fetches all Timeslots",
          "parameters": [
            {
              "paramType": "query",
              "name": "day",
              "type": "string",
              "description": "Sunday, Monday, …, Friday, Saturday",
              "required": false
            },
            {
              "paramType": "query",
              "name": "start_time",
              "type": "integer",
              "description": "0000, 0030, …, 2300, 2330",
              "required": false
            }
          ],
          "method": "get",
          "nickname": "Populates#show"
        }
      ]
    }
  ]
}